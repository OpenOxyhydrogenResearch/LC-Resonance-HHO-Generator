                <mode id="0" desc="Automatic resonance scanning mode.">0</mode>
                <mode id="1" desc="Manual fine tuning mode.">1</mode>
            </AppModes>
            <ScreenObjects>
                <LCDCharacterLine num="0" max_char="16">
                    <DispIdentifier id="0" desc="Gate Trigger frequency identifier." xpos_min="0" xpos_max="0" xcount="1">G</DispIdentifier>
                    <DispFrequency id="1">
