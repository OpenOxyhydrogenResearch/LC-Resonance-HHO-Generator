                            <prefix id="2" desc="Frequency unit in mega hertz (MHz)">M</prefix>
                        </unitPrefix>
                        <dispUnit id="2" desc="Dispays unit of frequency i.e. hertz (Hz)" xpos_min="8" xpos_max="9" xcount="2">Hz</dispUnit>
                    </DispFrequency>
                    <DispIdentifier id="2" desc="Resonance frequency lock identicator." xpos_min="10" xpos_max="10" xcount="1">[CUSTOM_LOCK]</DispIdentifier>
                    <DispIdentifier id="3" desc="Resonance output voltage identifier." xpos_min="11" xpos_max="11" xcount="1">O</DispIdentifier>
                    <DispVoltage id="4">
                        <voltageValue id="0" desc="Pulse output voltage value." xpos_min="12" xpos_max="14" xcount="3">9.9</voltageValue>
