            <AppModes>
                <mode id="0" desc="Boot up sequence.">0</mode>
            </AppModes>
            <ScreenObjects>
                <LCDCharacterLine num="0" max_char="16">
                    <DispTitle id="0">
                        <textObj id="0" desc="Displays Water Molecule icon." xpos_min="0" xpos_max="0" xcount="1">[CUSTOM_WATER_MOLECULE]</textObj>
                        <textObj id="1" desc="Application screen title." xpos_min="1" xpos_max="14" xcount="14" align="center">SCANNING RANGE</textObj>
